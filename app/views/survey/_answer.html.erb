<!Render the question, and then page depending on type>
<br><br><%=q.text%>
<%if q.type=="short_answer"%>
<%= render "short", q: q %>
<%end%>
<%if q.type=="single_choice"%>
<%= render "single", q: q %>
<%end%>
<%if q.type=="multi_choice"%>
<%= render "multi", q: q %>
<%end%>
<%if q.type=="sub_questions"%>
<%= render "sub", q: q %>
<%end%>
